{
  "name": "mobile-wallet-cri-test-harness",
  "version": "1.0.0",
  "description": "A service for testing a Wallet CRI",
  "author": "",
  "license": "MIT",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "execute": "node ./dist/src/server.js",
    "start": "npm run build && npm run execute",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "format": "prettier --ignore-path .gitignore --write \"*/**/*.{ts,json}\"",
    "test": "node_modules/.bin/jest  --testPathPattern=test/credential-issuer-tests.test.ts --coverage=false",
    "test:unit": "TZ=UTC jest node_modules/.bin/jest --testPathPattern=test/helpers src/config --silent"
  },
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.5"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.4",
    "bs58": "^6.0.0",
    "express": "^4.21.0",
    "jose": "^5.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.8.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.4.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
