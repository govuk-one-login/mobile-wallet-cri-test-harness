{
  "name": "mobile-wallet-cri-test-harness",
  "version": "1.0.0",
  "description": "A service for testing a Wallet CRI",
  "author": "",
  "license": "MIT",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "execute": "node ./dist/src/server.js",
    "start": "npm run build && npm run execute",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --write \"*/**/*.{ts,json}\"",
    "test": "node_modules/.bin/jest  --testPathPattern=test/credential-issuer-tests.test.ts --coverage=false",
    "test:unit": "TZ=UTC jest node_modules/.bin/jest --testPathPattern=test/helpers src/config --silent"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.8.4",
    "bs58": "^6.0.0",
    "express": "^5.1.0",
    "jose": "^5.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
